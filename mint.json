    },
    {
      "group": "CMS",
      "pages": [
        "cms/pages"
      ]
    },
    {
      "group": "Ecommerce",
      "pages": [
        "ecommerce/overview",
        "ecommerce/cart",
        "ecommerce/orders",
        "ecommerce/examples"
      ]
    },
    {
      "group": "Essentials",
      "pages": [
        "essentials/packages",
        "essentials/multi-language",
        "essentials/events",
        "essentials/package-helpers",
        "essentials/routes"
      ]
    }
  ]
}
