{
  "$schema": "https://mintlify.com/schema.json",
  "name": "Firepit CMS",
  "logo": {
    "dark": "/logo/logo.svg",
    "light": "/logo/logo.svg"
  },
  "favicon": "/favicon.svg",
  "colors": {
    "primary": "#F70",
    "light": "#F2D7CC",
    "dark": "#4A1420",
    "anchors": {
      "from": "#F70",
      "to": "#F70"
    }
  },
  "anchors": [
    {
      "name": "GitHub repository",
      "icon": "github",
      "url": "https://github.com/WebBuildsNL/firepit-cms"
    }
  ],
  "navigation": [
    {
      "group": "Get Started",
      "pages": [
        "introduction",
        "installation",
        "setting-up",
        "configuration",
        "upgrading"
